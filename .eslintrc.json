{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["security", "jest"],
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended",
    "plugin:node/recommended",
    "plugin:security/recommended"
  ],
  "env": {
    "node": true
  },
  "globals": {
    "jest": true
  },
  "rules": {
    "strict": 0,
    "node/no-unsupported-features/es-syntax": 0,
    "import/no-extraneous-dependencies": 0,
    "node/no-unpublished-require": 0
  }
}
